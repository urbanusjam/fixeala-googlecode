	<subDataset name="New Dataset 1" uuid="4b51d657-616e-47d0-b640-d5169de99bc0">
		<queryString>
			<![CDATA[SELECT
	i.ID_ISSUE as id,
	i.REPORTED_DATE as date,
	i.TITLE as title,
	i.ADDRESS as address,
	i.CITY as city,
	i.PROVINCE as province,
	u.USERNAME as username,
	i.STATUS as status
FROM issue i
INNER JOIN user u
ON u.ID_USER = i.ID_REPORTER
ORDER BY date desc]]>
