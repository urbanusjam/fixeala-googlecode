<div id="content">	
			<!-- Password Reset -->
			<div class="page-header">
    	 	<h4><i class="icon-user"></i>&nbsp;&nbsp;<i class="icon-angle-right"></i>&nbsp;&nbsp;Reseteo de contraseña</h4>    	 	
    		</div>   
			<form id="resetCredentialsForm" class="well span5">
			    <div class="row">			
					<div class="input-prepend">
			  			<span class="add-on"><i class="icon-lock"></i></span>
			  			<input type="password" id="newPassword" name="newPassword" placeholder="Nueva contraseña" class="input-xlarge">
			  		</div>
		  			<div class="input-prepend">
		  				<span class="add-on"><i class="icon-lock"></i></span>
		  				<input type="password" id="newPasswordConfirm" name="newPasswordConfirm" placeholder="Confirme nueva contraseña" class="input-xlarge">
		  			</div>
		  			<div class="span3" > 				
				      	<button type="submit" id="btnResetPass" class="btn btn-primary">Confirmar</button>&nbsp;&nbsp;		
				    	<button type="reset" class="btn">Cancelar</button>				    	
	    			</div>				     
			    </div> 
			    <div class="row" style="height:50px;">
				    	<div id="alert_placeholder"></div>		    
				</div>
	   		</form>
</div>
		